{
  "name": "github-contribution-growth-graph",
  "version": "0.0.0",
  "main": "dist/server.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "generate:logging:openapi": "swagger2openapi ./node_modules/@qkitzero/logging-service-openapi/gen/openapi.json -o ./src/infrastructure/api/logging/openapi.json",
    "generate:logging:schema": "openapi-typescript ./src/infrastructure/api/logging/openapi.json -o ./src/infrastructure/api/logging/schema.ts",
    "generate:all": "npm run generate:logging:openapi && npm run generate:logging:schema",
    "build": "rimraf dist && tsc",
    "start": "node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "test": "jest",
    "test:cov": "jest --coverage",
    "coverage:open": "open coverage/lcov-report/index.html"
  },
  "dependencies": {
    "chart.js": "^4.5.1",
    "chartjs-node-canvas": "^5.0.0",
    "express": "^5.2.0",
    "graphql-request": "^7.3.5",
    "openapi-fetch": "^0.15.0"
  },
  "devDependencies": {
    "@qkitzero/logging-service-openapi": "^1.3.2",
    "@tsconfig/node24": "^24.0.3",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.1",
    "jest": "^30.2.0",
    "openapi-typescript": "^7.10.1",
    "prettier": "^3.7.3",
    "prettier-plugin-organize-imports": "^4.3.0",
    "rimraf": "^6.1.2",
    "swagger2openapi": "^7.0.8",
    "ts-jest": "^29.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
